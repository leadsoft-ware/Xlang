import "lib/libxlang.xasm":xasm;

set db_size=1048576


// 通过getBytecodeLocation取得函数地址,然后返回给fib，然后再计算右值的函数块

vmbyte* fib = function none int (x:int){
    if(x <= 2){
        return x;
    };
    return fib(x-1) + fib(x-2);
};



vmbyte* main = function norefernce int (argc:int,argv:char*){
    int i = 0,j = 1;
    i = i + 1;
    i = j + i;
    i *= i;
    return i;
};